    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "bits-ui": "^2.11.0",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.1",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
